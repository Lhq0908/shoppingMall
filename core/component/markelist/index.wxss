.list-li{display:flex;align-items:flex-start;background:#FFF;padding:20rpx;border-radius:10rpx;margin-bottom:20rpx}.list-li .li-l{width:188rpx;height:188rpx;border-radius:10rpx;margin-right:18rpx}.list-li .li-l image{width:100%;height:100%}.list-li .li-r{flex:1}.list-li .li-r .title{font-size:28rpx;color:#333;display:-webkit-box;text-overflow:ellipsis;overflow:hidden;-webkit-line-clamp:1;-webkit-box-orient:vertical;height:35rpx}.list-li .li-r .number-item{display:flex;align-items:center;margin-top:15rpx}.list-li .li-r .number-item .item-l text{width:60rpx;display:block;text-align:center;font-size:21rpx;color:#999999}.list-li .li-r .number-item .progress{flex:1;margin:0 23rpx;border:1px solid #97c155;border-radius:30rpx}.list-li .li-r .price{font-size:24rpx;color:#333333;display:flex;align-items:center;justify-content:space-between;margin-top:10rpx}.list-li .li-r .price view text{font-size:28rpx;color:#f82300}.list-li .li-r .price view .grey{font-size:28rpx;color:#999;text-decoration:line-through;margin-left:10rpx}.list-li .li-r .time{display:flex;align-items:center;justify-content:space-between;font-size:20rpx;margin-top:12rpx}.list-li .li-r .time .time-item{color:#999999}
